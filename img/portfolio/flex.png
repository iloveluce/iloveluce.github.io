<!-- instruments specific ids that redirect user to main application web app with anon segment id for use across apps -->
<script>
function waitForLocalStorageValue(key) {
  return new Promise(function(resolve) {
    function checkValue() {
      var value = localStorage.getItem(key);

      if (value) {
        resolve(value); 
      } else {
        setTimeout(checkValue, 500); // Retry after 500 milliseconds
      }
    }

    checkValue(); // Initial check
  });
}
  
  
const el = document.querySelectorAll('[data-marketing="register-redirect"]')
console.log('elements found from query selector:' + el.length);
console.log(el);
if (el) {
  // console.log('looping through registration elements');
  // console.log('local storage number of entries ' + localStorage.length);
  // console.log(localStorage);

  // let params = ["utm_source", "utm_medium", "utm_campaign", "utm_adgroup", "utm_content", "utm_lpurl", "utm_referrer", "ajs_anonymous_id", "variant"];
  let params = ["ajs_anonymous_id", "variant"];
  let urlSearchP = {};
  
  waitForLocalStorageValue('ajs_anonymous_id').then(function(){
    // calculate the search url
    // console.log('this is the object entries for local storage with length ' + localStorage.length);
    // console.log(localStorage);
	for (let [key, value] of Object.entries(localStorage)) {
    // if variable is of a specific value, add it to the dictionary
    if (params.includes(key)) {
      if (key === 'ajs_anonymous_id') {
        // console.log('id is ' + key + ' ' + value);
        // make it special so it doesnt collide with anon id of main flow
        urlSearchP['segment_anon'] = value.replace(/['"]+/g, '');
      }
      urlSearchP[key] = value.replace(/['"]+/g, '');
    }
  }
  // convert dictionary to url search param string
  let searchParams = new URLSearchParams(urlSearchP).toString();
  // console.log(searchParams);
  // console.log('number of elements with registration redirects ' + el.length);
  // iterate and attach href updates on all elements
  for (let elIndex = 0; elIndex < el.length; elIndex++) {
    if (window.location.host.includes("staging-flexbase.webflow.io")) {
      el[elIndex].href = "https://dev.flexbase.app/register?" + searchParams;
      // console.log('staging element href ' + el[elIndex]);
    } else {
      // add string to href      
      const searchParamsArray = el[elIndex].href.split('?');
      if (searchParamsArray.length > 1 && searchParamsArray[1] !== '') {
 		 // console.log('params found');
        el[elIndex].href += "&" + searchParams;
	  } else {
      	el[elIndex].href += "?" + searchParams;
      }
      
      // console.log('production element href ' + el[elIndex]);
    }

    el[elIndex].addEventListener("click", function() {
      // console.log('firing started registration event');
      analytics.track("Started Registration", {
        "url": window.location.href.split('?')[0],
        "href": el.href,
        "referrer": document.referrer
      });
    });
  }
  }).catch(function() {
    console.log('localstorage ajs_anonymous_id could not be found');
  });  
};
</script>

